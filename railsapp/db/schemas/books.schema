# -*- mode: ruby -*-
# vi: set ft=ruby :
create_table "books", charset: "utf8mb4", collation: "utf8mb4_0900_ai_ci", force: :cascade do |t|
  t.string "title", null: false
  t.datetime "created_at", null: false
  t.datetime "updated_at", null: false
  t.string "author"
  t.text "description"
  t.string "image_link"
end
